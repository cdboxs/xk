/* pages/package/results/index.wxss */

page {
  background: #f4f4f4;
}
.iconfont {
  font-size: 40rpx;
}
.selectTime{
  width: 100%;
  height: 100rpx;
  line-height: 100rpx;
  background: #fff;
  font-size: 34rpx;
  color: #0282f5;
  border-bottom:1rpx solid #ddd;
}
.selectTime view{
  display: flex;
  flex-flow: row;
  width: 100%;
  justify-content: space-around;
}
.results {
  width: 100%;
  background: #fff;
  margin: 20rpx 0;
  padding-bottom:20rpx;
}

.title {
  width: 100%;
  height: 80rpx;
  display: flex;
  justify-content: flex-start;
  align-items: center;
  border-bottom:1rpx solid #eee;
  font-size: 30rpx;
  color: #333;
  font-weight: 600;
  padding-left:5%;
  box-sizing: border-box;
}


.list {
  width: 100%;
  display: flex;
  justify-content: space-around;
  margin: 15rpx 0 30rpx 0;
}

.list1 {
  width: 25%;
  font-size: 30rpx;
  color: #333;
  text-align: center;
}

.list1 text {
  display: block;
  text-align: center;
  padding-top: 10rpx;
}

.total {
  display: flex;
  justify-content: space-around;
}

.total text{
  display: block;
  width: 25%;
  text-align: center;
  color: #0282f5;
  font-size: 28rpx;
}
