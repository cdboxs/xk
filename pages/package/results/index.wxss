/* pages/package/results/index.wxss */

page {
  background: #f4f4f4;
}

.results {
  width: 100%;
  height: 270rpx;
  background: #fff;
  margin-bottom: 20rpx;
}

.title {
  width: 100%;
  height: 80rpx;
  display: flex;
  justify-content: space-around;
  align-items: center;
}

.txt1 {
  display: inline-block;
  width: 400rpx;
  font-size: 32rpx;
  color: #333;
  font-weight: 600;
}

.txt2 {
  font-size: 28rpx;
}

.list {
  width: 100%;
  display: flex;
  justify-content: space-around;
  margin-top: 35rpx;
}

.list1 {
  font-size: 26rpx;
  color: #333;
  display: inline;
}

.list1 text {
  display: block;
}

.total {
  margin-right: 30rpx;
  line-height: 100rpx;
}

.list3 {
  display: flex;
  justify-content: flex-end;
  color: #fb7c0b;
  font-size: 34rpx;
}
